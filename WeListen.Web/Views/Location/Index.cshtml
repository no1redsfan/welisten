@model WeListen.Web.Viewmodels.Locations.Index

<h2>Locations/Events</h2>

<table>
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model.Locations) {
    <tr>
        <td>
            @item.Name
            @*@Html.ActionLink("Enter Location", "Index", new { id=item.LocationId  }) *@
            <input type="submit" value="Enter Location" onclick="location.href='@Url.Action("Home", "Location", new {locationId=@item.LocationId})'" />
            
        </td>
    </tr>
}
</table>
